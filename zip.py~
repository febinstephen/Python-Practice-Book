a = [1,2,3]
b = [3,3,3,2,4]
#zip function
output = [(a[i],b[i]) for i in range(min(len(a),len(b)))]
print output

#map function
def square(x):
	return x*x
maplist = []
for i in a:
	maplist.append(square(i))
print maplist 

#filter function

def two(x):
	if x%2 == 0:
		return True
	else:
		return False

c = []	
for i in b:
	 if two(i):
	 	c.append(i)
print c	


#triplets function


n = 10
out = [(x,y,z) for x in range(1,n) for y in range(x,n) for z in range(y,n) if x+y == z]
print out

#enumerate fun

l = len(b)
for i in range(l):
	print "%d\t%d\n"%(i,b[i])
 

# array function
r = 2
f = 3
array = [ [0 for i in range(f)] for j in range(r)]


print array

array[0][0] = 1

print array
