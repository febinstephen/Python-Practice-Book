a = ["a.py","a.c","b.txt","b.py"]
n = len(a)
a1,a2 = [],[]
b = []
new = [] 
for i in range(n):
	for j in range(i+1,n):
		a1 = a[i].split('.')
		a2 = a[j].split('.')
		if a1[1] > a2[1]:
			temp = a[i]
			a[i] = a[j]
			a[j] = temp 
		
print a



