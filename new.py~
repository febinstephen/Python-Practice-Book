size = 3
a = [1,2,3,4,5,6]
temp_list = []
result_list = []
length = len(a)
print length
for  i in range(length):
	
	if i < size:
		temp_list.append(a[i])
		print temp_list
		
	else:
		result_list.append(temp_list)
		#print temp_list
		del temp_list[:]
		i = i-1
		print i
		size = size+3		
print result_list
print a
 


